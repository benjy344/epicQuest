<p id="notice"><%= notice %></p>

<h1>Chests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chests.each do |chest| %>
      <tr>
        <td><%= link_to 'Show', chest %></td>
        <td><%= link_to 'Edit', edit_chest_path(chest) %></td>
        <td><%= link_to 'Destroy', chest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chest', new_chest_path %>
